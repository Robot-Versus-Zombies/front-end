{
	"compilerOptions": {
		"jsx": "react",
		"target": "es2016",
		"module": "esnext",
		"outDir": "./dist",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"allowJs": true,
		"skipLibCheck": true,
		"lib": ["dom", "dom.iterable", "esnext"],
		"allowSyntheticDefaultImports": true,
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"noEmit": true
	},
	"include": ["src"]
}
